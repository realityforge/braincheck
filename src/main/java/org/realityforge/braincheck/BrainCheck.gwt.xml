<module>
  <inherits name='jsinterop.base.Base'/>

  <source path='' includes='*.java'/>

  <!--
    These properties are all defined in org.realityforge.braincheck.BrainCheckConfig
    See that class for a better understanding of what these properties do.
  -->

  <define-property name='braincheck.environment' values='development,production'/>
  <set-property name='braincheck.environment' value='production'/>

  <define-property name='braincheck.verbose_error_messages' values='true,false'/>
  <set-property name='braincheck.verbose_error_messages' value='false'/>

  <define-property name='braincheck.check_invariants' values='true,false'/>
  <set-property name='braincheck.check_invariants' value='false'/>

  <define-property name='braincheck.check_api_invariants' values='true,false'/>
  <set-property name='braincheck.check_api_invariants' value='false'/>
</module>
